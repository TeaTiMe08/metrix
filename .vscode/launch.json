        "TYPING_CHARACTER": "█",
        "ACTIVITY": "true",
        "ACTIVITY_TEXT": "Last month commit activity:",
        "ACTIVITY_DAYS": "30",
        "FPS": "50",
        "LOOP": "false",
        "WIDTH": "622",
        "HEIGHT": "356",
        "GLITCHES": "true",
        "MAX_GLITCHES": "4",
        "GLITCH_PROBABILITY": "3",
