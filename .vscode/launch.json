  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Debug",
      "type": "debugpy",
      "request": "launch",
      "program": "src/main.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": [],
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src",
        "DEBUG_MODE": "true",
        "LOG_LEVEL": "INFO",
        "FFMPEG_LOG_LEVEL": "ERROR",
        "GITHUB_USERNAME": "joanroig",
        "GITHUB_TOKEN": "",
        "FONT_SIZE": "20",
        "SYMBOL_FONT_SIZE": "20",
        "FONT_PATH": "assets/MxPlus_IBM_BIOS.ttf",
        "SYMBOL_FONT_PATH": "assets/MxPlus_IBM_BIOS.ttf",
        "TEXT_COLOR": "random",
        "BACKGROUND_COLOR": "random",
        "MINIMUM_CONTRAST": "2",
